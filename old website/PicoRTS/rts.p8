--ship logic

function makeship(_x,_y,_hp,_wtype,
_stype)
local _spd=0;
local _c=0;
if(_stype==1)then _spd=10 _c=1000
elseif(_stype==2)then _spd=5 _c=300
elseif(_stype==3)then _spd=20 _c=3000
elseif(_stype==4)then _spd=25 _c=10000
elseif(_stype==6)then _spd=30 _c=25000 end
sid=count(ships)+1;
add(ships,{x=_x,y=_y,hp=_hp,
wtype=_wtype,stype=_stype,
dx=_x,dy=_y,id=sid,spd=_spd,cost=_c})

end

function shipupd()
for s in all(ships) do
	if(frm%s.spd==0)then
		if(s.x<s.dx)then
		s.x+=1;
		elseif(s.x>s.dx)then
		s.x-=1;
		end
		if(s.y<s.dy)then
		s.y+=1;
		elseif(s.y>s.dy)then
		s.y-=1;
		end
	elseif(frm==30)then frm=0 end
	
	
	end
end
